programa {
  funcao inicio() {
    // declaração de vetor
    cadeia nomes[] = {"HTML", "CSS", "Cavalairos do Zodíaco", "Dragon Ball", "Python", "Java"}

    // exibe na tela o primeiro nome da lista
    escreva(nomes[0],"\n")

    // exibe todos os nomes da lista
    para (inteiro i = 0; i < 5; i++)
    {
      escreva(nomes[i], "\n")
    }

  }
}
